{
  "name": "joyoung-web",
  "version": "1.1.0",
  "description": "pc模板",
  "author": "",
  "scripts": {
    "cz": "git add . && git cz",
    "dev": "vue-cli-service serve --open",
    "prod": "vue-cli-service serve --mode production",
    "test": "vue-cli-service serve --mode test",
    "build:prod": "vue-cli-service build --mode prod && rm -fr ./dist/**/**/*.map",
    "development": "vue-cli-service build --mode develop",
    "testing": "vue-cli-service build --mode test",
    "preview": "node build/index.js --preview",
    "eslint": "eslint --ext .js,.vue src/**"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@base/eslint-config": "^1.0.3",
    "@base/file": "^1.0.7",
    "@base/jy-point": "^1.0.9",
    "@base/jy-ui-plus": "^1.0.4",
    "@base/jy-watermark": "^1.0.3",
    "@base/utils": "^1.1.0",
    "axios": "0.18.1",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.11.7",
    "echarts": "^5.2.1",
    "js-cookie": "2.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "qs": "^6.9.1",
    "sass-loader": "^11.0.1",
    "screenfull": "4.2.0",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "v-viewer": "^1.6.4",
    "vue": "^2.6.12",
    "vue-pdf": "^4.3.0",
    "vue-print-nb": "^1.7.5",
    "vue-router": "^3.5.1",
    "vue2-org-tree": "^1.3.6",
    "vuex": "^3.6.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.2/xlsx-0.19.2.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@commitlint/cli": "^12.0.1",
    "@vue/cli-plugin-babel": "3.5.3",
    "@vue/cli-service": "3.5.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.2.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^5.15.3",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.3.5",
    "less": "^3.13.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.5.4",
    "svg-sprite-loader": "4.1.3",
    "vue-template-compiler": "^2.6.12"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
